  "sync.syncExtensions": true,
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.shell.linux": "/usr/local/bin/zsh",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.fontSize": 20,
